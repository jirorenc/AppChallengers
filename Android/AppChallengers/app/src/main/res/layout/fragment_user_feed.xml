<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="center"
    android:gravity="center"
    android:orientation="vertical"
    tools:context="com.appchallengers.appchallengers.fragments.main.UserFeedFragment">

    <com.victor.loading.rotate.RotateLoading
        android:id="@+id/user_feed_fragment_rotateloading"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_centerInParent="true"
        android:layout_marginTop="10dp"
        app:loading_color="#FD5739"
        app:loading_width="2dp" />

    <com.appchallengers.appchallengers.helpers.component.SFUITextRegularTextViewComponent
        android:id="@+id/user_feed_new_load_message"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="İşleminiz tamamlanıyor, tamamlandığında burada görülecektir."
        android:visibility="gone" />

    <im.ene.toro.widget.Container xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/player_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />


</LinearLayout>
